set GD_Orchid_BossWeapons.Pistol.Pistol_Barrel_ScarletsGreed GestaltModeSkeletalMeshName Pistol_Barrel_Dahl
set GD_Orchid_BossWeapons.Pistol.Pistol_Barrel_ScarletsGreed rarity (BaseValueConstant=5.000000,BaseValueAttribute=None,InitializationDefinition=None,BaseValueScaleConstant=1.000000)

